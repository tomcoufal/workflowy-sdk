{
    "name": "tomcoufal/workflowy-sdk",
    "description": "Modern PHP SDK for Workflowy API (Laravel 12 ready)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomco",
            "email": "dev@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "pestphp/pest": "^2.0",
        "guzzlehttp/guzzle": "^7.8"
    },
    "autoload": {
        "psr-4": {
            "Workflowy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Workflowy\\Laravel\\WorkflowyServiceProvider"
            ],
            "aliases": {
                "Workflowy": "Workflowy\\Laravel\\Facades\\Workflowy"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable"
}
